{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": [
            "dom",
            "esnext"
        ],
        "jsx": "react",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    }
}
